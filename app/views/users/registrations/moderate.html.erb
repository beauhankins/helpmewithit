<h1>Users</h1>

<table>
	<tr>
      <th>Name</th>
	  <th>Email</th>
	  <th>Approved?</th>
	</tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %> </td>
      <td><%= user.email %> </td>
      <td><%= user.approved %> </td>
      <td><%= link_to("Approve", approve_user_path(user.id)) %></td>
    </tr>
  <% end %>
</table>